        <service id="bankholiday_repository.doctrine" class="%bankholiday_repository.doctrine.class%" factory-service="doctrine" factory-method="getrepository">
            <argument type="string">%bankholiday.doctrine.entity%</argument>
        </service>

        <!-- AppImpl Services -->
        <service id="addressquotahelper_repository" alias="addressquotahelper_repository.doctrine"></service>
        <service id="addressquotahelper_repository.doctrine" class="%addressquotahelper_repository.doctrine.class%" factory-service="doctrine" factory-method="getrepository">
            <argument type="string">%addressquotahelper.doctrine.entity%</argument>
        </service>
        
    </services>

</container>
