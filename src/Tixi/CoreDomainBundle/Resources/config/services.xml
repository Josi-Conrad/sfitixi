        <service id="repeateddrivingorderplan_repository" alias="repeateddrivingorderplan_repository.doctrine"></service>
        <service id="repeateddrivingorderplan_repository.doctrine" class="%repeateddrivingorderplan_repository.doctrine.class%" factory-service="doctrine" factory-method="getrepository">
            <argument type="string">%repeateddrivingorderplan.doctrine.entity%</argument>
        </service>

        <service id="drivingpool_repository" alias="drivingpool_repository.doctrine"></service>
        <service id="drivingpool_repository.doctrine" class="%drivingpool_repository.doctrine.class%" factory-service="doctrine" factory-method="getrepository">
            <argument type="string">%drivingpool.doctrine.entity%</argument>
        </service>

        <service id="repeateddrivingassertionplan_repository" alias="repeateddrivingassertionplan_repository.doctrine"></service>
