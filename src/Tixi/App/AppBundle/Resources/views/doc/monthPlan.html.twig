<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="/web/css/bootstrap.css">
    <link rel="stylesheet" href="/web/css/tixiBootstrapTheme.css">
    <link rel="stylesheet" href="/web/css/main.css">
</head>
<body>
<div class="container">
    <div>
        <div class="pull-left text-left">
            <h2 class="text-muted">
                iTIXI  {{ title_parameter_name }}  {{ title_parameter_month }}
            </h2>
        </div>
    </div>
    <div class="clearfix"></div>
    <hr>
    <div>
        <div>
            <br>
        </div>
        <div>
            {% for dayTitle, dayAndShift in dayAndShifts %}
                <h3>
                    {{ dayTitle }}
                </h3>

                {% for shiftName, shiftAndDriver in dayAndShift %}
                    <div>
                        {{ shiftName }}
                    </div>

                    {% for driverName in shiftAndDriver %}
                        <div>{{ driverName }}</div>
                    {% endfor %}

                {% endfor %}
            {% endfor %}
        </div>
        <div>
            <br><br>
        </div>
    </div>
    <div>
        <hr>
        <footer>
            <p>
                &copy;
                <a id="fpx_footeraction_about">
                    {{ tixi_parameter_app }} {{ tixi_parameter_version }}
                </a>
                | {% trans %}main.mandant{% endtrans %}
                <a id="fpx_footeraction_about">
                    {{ tixi_parameter_client }}
                </a>
            </p>
        </footer>
    </div>
</div>
</body>
</html>
