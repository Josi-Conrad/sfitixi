<div class="panel {% if type == 'primary' %}panel-primary{% else %}panel-default{% endif %}">
    <div class="panel-heading"><h3 class="panel-title">{{ headerDisplayText }}</h3></div>
    <div class="panel-body">
        {% if children is defined %}
            <div class="col-md-12">
                {% for resolvedElement in children %}
                        {{ resolvedElement.rawData | raw }}
                {% endfor %}
            </div>
        {% endif %}
    </div>
</div>





