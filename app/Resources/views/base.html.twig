{% set menuId = menuId|default('') %}
<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {% set titleDisplayText = titleDisplayText|default('App') %}
    {% set titleDisplaySubText = titleDisplaySubText|default('') %}
    <title>{{ tixi_parameter_app }} - {{ titleDisplayText|trans }} {{ titleDisplaySubText|trans }}</title>
    <meta name="description" content="">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('css/tixiBootstrapTheme.css') }}">
    <link rel="stylesheet" href="{{ asset('css/main.css') }}">
    <link rel="stylesheet" href="{{ asset('css/jqueryui/ui-lightness/jquery-ui-1.10.4.custom.css') }}">
    {% block stylesheetcustom %}{% endblock %}
    <script src="{{ asset('js/jquery-1.10.1.js') }}"></script>
    <script src="{{ asset('js/jqueryui/jquery-ui-1.10.4.custom.js') }}"></script>
</head>
<body>
<div class="container">
    <div>
        <div class="pull-left text-left">
            <h3 class="text-muted">
                <img src="{{ asset('images/logo32px.png') }}" alt="Logo iTIXI"/>
                {{ tixi_parameter_app }} {{ tixi_parameter_client }}
            </h3>
        </div>
        <div class="pull-right text-right">
            {% include '::topAction.html.twig' %}
        </div>
    </div>
    <div class="clearfix"></div>
    <div>
        {{ renderMenu(menuId) }}
    </div>
    <div class="row">
        {{ apy_breadcrumb_trail_render() }}
    </div>
    <div>
        {% block body %}{% endblock %}
    </div>
    <div>
        <hr>
        <footer>
            <p>
                &copy; {{ tixi_parameter_app }} {{ tixi_parameter_client }}
                |
                <a id="fpx_footeraction_about" href="{{ path('tixiapi_about') }}">
                    {{ tixi_parameter_app }} {{ tixi_parameter_version }}
                </a>
                 |
                <a id="fpx_footeraction_support" href="{{ path('tixiapi_support') }}">
                    {% trans %}button.support{% endtrans %}
                </a>
            </p>
        </footer>
    </div>
</div>

<!-- /container -->
<script src="{{ asset('js/bootstrap.js') }}"></script>
<script src="{{ asset('js/main.js') }}"></script>
<script src="{{ asset('bundles/tixiapi/js/FormValidationController.js') }}"></script>
<script src="{{ asset('bundles/tixiapi/js/dataGrid.js') }}"></script>

{% block javascriptscustom %}{% endblock %}

</body>
</html>
