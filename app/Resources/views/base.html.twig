{% set menuId = menuId|default('home') %}
<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {% set titleDisplayText = titleDisplayText|default('App') %}
    {% set titleDisplaySubText = titleDisplaySubText|default('') %}
    <title>iTixi - {{ titleDisplayText|trans }} {{ titleDisplaySubText|trans }}</title>
    <meta name="description" content="">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('css/tixiBootstrapTheme.css') }}">
    <link rel="stylesheet" href="{{ asset('css/main.css') }}">
    <link rel="stylesheet" href="{{ asset('css/jqueryui/ui-lightness/jquery-ui-1.10.4.custom.css') }}">
    {% block stylesheetcustom %}{% endblock %}
    <script src="{{ asset('js/jquery-1.10.1.js') }}"></script>
    <script src="{{ asset('js/jqueryui/jquery-ui-1.10.4.custom.js') }}"></script>
</head>
<body>
<div class="container">
    <div>
        <div class="pull-left text-left">
            <h3 class="text-muted">
                <img src="{{ asset('images/logo32px.png') }}" alt="Logo iTIXI"/>
                iTixi 2.0.2
            </h3>
        </div>
        <div class="pull-right text-right">
            {% include '::topAction.html.twig' %}
        </div>
    </div>
    <div class="clearfix"></div>
    <div>
        {{ renderMenu(menuId) }}
        {#{% include '::topNavigation.html.twig' %}#}
    </div>
    <div class="row">
        {{ apy_breadcrumb_trail_render() }}
    </div>
    <div>
        {% block body %}{% endblock %}
    </div>
    <div>
        <hr>
        <footer>
            <p>&copy; Company 2013</p>
        </footer>
    </div>
</div>

<!-- /container -->
<script src="{{ asset('js/bootstrap.js') }}"></script>
<script src="{{ asset('js/main.js') }}"></script>
<script src="{{ asset('bundles/tixiapi/js/FormValidationController.js') }}"></script>
<script src="{{ asset('bundles/tixiapi/js/dataGrid.js') }}"></script>

{% block javascriptscustom %}{% endblock %}

</body>
</html>
